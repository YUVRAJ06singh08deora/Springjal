<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#15151F"
    tools:context=".Admin_Home">
    <ImageView
        android:id="@+id/userphoto"
        android:layout_width="146dp"
        android:layout_height="113dp"
        android:src="@drawable/patient1"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="125dp"/>
    <TextView
        android:id="@+id/username"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Walter White"
        android:textSize="28dp"
        android:textColor="#fff"
        android:textStyle="bold"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="120dp"
        android:layout_below="@+id/userphoto"/>
    <TextView
        android:id="@+id/userid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Emp Id: 20MIS0196"
        android:textColor="#fff"
        android:textSize="14dp"
        android:layout_marginLeft="139dp"
        android:layout_below="@+id/username"/>
    <TextView
        android:id="@+id/role"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Role: Admin"
        android:textColor="#fff"
        android:textSize="14dp"
        android:layout_marginLeft="139dp"
        android:layout_below="@+id/userid"/>
    <RelativeLayout
        android:id="@+id/recievedActivity"
        android:layout_width="369dp"
        android:layout_height="77dp"
        android:layout_below="@+id/role"
        android:layout_marginLeft="30dp">
        <ImageView
            android:layout_width="56dp"
            android:layout_height="56dp"
            android:src="@drawable/new_activity"
            android:layout_marginTop="13dp"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=" Recieved Activity"
            android:textStyle="bold"
            android:textSize="20dp"
            android:textColor="#fff"
            android:layout_marginLeft="90dp"
            android:layout_marginTop="28dp"/>
        <ImageView
            android:layout_width="32dp"
            android:layout_height="27dp"
            app:srcCompat="@drawable/red_arrow"
            android:layout_marginLeft="319dp"
            android:layout_marginTop="28dp"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="59dp"
            android:textColor="#9A8787"
            android:text="______________________________________________________"/>

    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/analysespringshed"
        android:layout_width="369dp"
        android:layout_height="77dp"
        android:layout_below="@+id/recievedActivity"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="10dp">
        <ImageView
            android:layout_width="56dp"
            android:layout_height="56dp"
            android:src="@drawable/resume_activities"
            android:layout_marginTop="13dp"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=" Analyse Springshed"
            android:textStyle="bold"
            android:textSize="20dp"
            android:textColor="#fff"
            android:layout_marginLeft="90dp"
            android:layout_marginTop="28dp"/>
        <ImageView
            android:layout_width="32dp"
            android:layout_height="27dp"
            app:srcCompat="@drawable/red_arrow"
            android:layout_marginLeft="319dp"
            android:layout_marginTop="28dp"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="59dp"
            android:textColor="#9A8787"
            android:text="______________________________________________________"/>
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/analytics"
        android:layout_width="369dp"
        android:layout_height="77dp"
        android:layout_below="@+id/analysespringshed"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="10dp">
        <ImageView
            android:layout_width="56dp"
            android:layout_height="56dp"
            android:src="@drawable/checkstatus"
            android:layout_marginTop="13dp"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Analytics"
            android:textStyle="bold"
            android:textSize="20dp"
            android:textColor="#fff"
            android:layout_marginLeft="90dp"
            android:layout_marginTop="28dp"/>
        <ImageView
            android:layout_width="32dp"
            android:layout_height="27dp"
            app:srcCompat="@drawable/red_arrow"
            android:layout_marginLeft="319dp"
            android:layout_marginTop="28dp"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="59dp"
            android:textColor="#9A8787"
            android:text="______________________________________________________"/>
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/spring_data_filter"
        android:layout_width="369dp"
        android:layout_height="77dp"
        android:layout_below="@+id/analytics"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="10dp">
        <ImageView
            android:layout_width="56dp"
            android:layout_height="56dp"
            android:src="@drawable/maps"
            android:layout_marginTop="13dp"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Spring Shed Data"
            android:textStyle="bold"
            android:textSize="20dp"
            android:textColor="#fff"
            android:layout_marginLeft="90dp"
            android:layout_marginTop="28dp"/>
        <ImageView
            android:layout_width="32dp"
            android:layout_height="27dp"
            app:srcCompat="@drawable/red_arrow"
            android:layout_marginLeft="319dp"
            android:layout_marginTop="28dp"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="59dp"
            android:textColor="#9A8787"
            android:text="______________________________________________________"/>
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/spring_plot_all"
        android:layout_width="369dp"
        android:layout_height="77dp"
        android:layout_below="@+id/spring_data_filter"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="10dp">
        <ImageView
            android:layout_width="56dp"
            android:layout_height="56dp"
            android:src="@drawable/feedback"
            android:layout_marginTop="13dp"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Feedback"
            android:textStyle="bold"
            android:textSize="20dp"
            android:textColor="#fff"
            android:layout_marginLeft="90dp"
            android:layout_marginTop="28dp"/>
        <ImageView
            android:layout_width="32dp"
            android:layout_height="27dp"
            app:srcCompat="@drawable/red_arrow"
            android:layout_marginLeft="319dp"
            android:layout_marginTop="28dp"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="59dp"
            android:textColor="#9A8787"
            android:text="______________________________________________________"/>
    </RelativeLayout>
</RelativeLayout>